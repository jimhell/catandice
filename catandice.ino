int a;
int b;
int w;
int s;
int button = 0;
int randv;
int clicks = 0;

void setup() {
  randv = random(1,100000);
  display.init();
  display.flipScreenVertically();
  display.setFont(ArialMT_Plain_16);
  display.clear();
  display.drawString(0,0,(String)randv);
  display.drawXbm(0, 1, 128, 61, catan);
  display.display();
  pinMode(button, INPUT_PULLUP);
  randomSeed(randv);
}

void loop() {
  if(digitalRead(button) == LOW) {
    clicks++;
    doit();
    delay(400);
  }
  a = random(1,7);
  b = random(1,7);
  w = random(1,7);
}

void doit() {
  display.clear();
  
  if(a == 1) { display.drawXbm(87, 3, 40, 40, action_blue); }
  if(a == 2) { display.drawXbm(87, 3, 40, 40, action_green); }
  if(a == 3) { display.drawXbm(87, 3, 40, 40, action_yellow); }
  if(a > 3) { display.drawXbm(87, 3, 40, 40, action_robber); }
  
  if(b == 1) { display.drawXbm(45, 3, 40, 40, w1); }
  if(b == 2) { display.drawXbm(45, 3, 40, 40, w2); }
  if(b == 3) { display.drawXbm(45, 3, 40, 40, w3); }
  if(b == 4) { display.drawXbm(45, 3, 40, 40, w4); }
  if(b == 5) { display.drawXbm(45, 3, 40, 40, w5); }
  if(b == 6) { display.drawXbm(45, 3, 40, 40, w6); }

  if(w == 1) { display.drawXbm(3, 3, 40, 40, b1); }
  if(w == 2) { display.drawXbm(3, 3, 40, 40, b2); }
  if(w == 3) { display.drawXbm(3, 3, 40, 40, b3); }
  if(w == 4) { display.drawXbm(3, 3, 40, 40, b4); }
  if(w == 5) { display.drawXbm(3, 3, 40, 40, b5); }
  if(w == 6) { display.drawXbm(3, 3, 40, 40, b6); }

  s = b + w;
  
  display.setTextAlignment(TEXT_ALIGN_LEFT);
  display.drawString(3, 48, (String)clicks);
  display.setTextAlignment(TEXT_ALIGN_RIGHT);
  display.drawString(125, 48, (String)w + " + " + (String)b + " = " + (String)s);
  
  display.display();
}
